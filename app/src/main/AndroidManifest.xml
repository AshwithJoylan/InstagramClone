<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.istagram.ashwith.instagramclone">
    
    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:allowBackup="true"
        android:icon="@drawable/insta_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/insta_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home.HomeActivity">
        </activity>
        <activity android:name=".Likes.LikesActivity" />
        <activity android:name=".Profile.ProfileActivity" />
        <activity android:name=".Search.SearchActivity" />
        <activity android:name=".Share.ShareActivity"></activity>
        <activity android:name=".Profile.AccountSettingsActivity"></activity>
        <activity android:name=".Login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login.RegisterActivity"></activity>
    </application>

</manifest>